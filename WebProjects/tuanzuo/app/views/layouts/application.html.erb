<!DOCTYPE html>
<html>
<head>
  <title>请叫我团座 | <%= yield(:title) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag "bootstrap", media: "all" %>
  <%= stylesheet_link_tag "custom", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "bootstrap" %>
  <%= javascript_include_tag "jquery-1.9.1" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header class="navbar navbar-inverse navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				</button>
				<a class="brand" href="#"><b>TuanZuo</b> <i>团座</i></a>
				<div class="nav-collapse collapse">
				<ul class="nav">
					<li><%= link_to "Home", '/home/index' %></li>
					<li><%= link_to "Blog", '#' %></li>
					<li><%= link_to "Photos", '#' %></li>
				</ul>
				</div>
			</div>
		</div>
	</header>
<div id="wrap">
<div class="container">
<%= yield %>
</div>
</div>

<div id="footer">
	<div class="container">
		<div class="muted credit">
			<%= link_to "About me", '/service/about' %> 
			<%= link_to "License", '/service/license' %> 
			<%= link_to "feedback", '/service/feedback' %> 
		</div>
	</div>
</div>
</body>
</html>
